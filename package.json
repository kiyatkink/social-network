  "scripts": {
    "prepare": "husky install config/husky",
    "start": "webpack serve",
    "start:dev": "concurrently \"yarn start\" \"yarn json-server:start\"",
    "build:prod": "webpack --env mode=prod",
    "build:dev": "webpack --env mode=dev",
    "json-server:start": "node ./json-server/index.js",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint:fix": "stylelint \"**/*.scss\" --fix",
    "test": "jest --config ./config/jest/jest.config.ts",
    "storybook": "storybook dev -p 6006 --no-open -c ./config/storybook",
    "build-storybook": "storybook build -c ./config/storybook",
    "loki:test": "loki test",
    "loki:approve": "loki approve",
    "loki:report": "reg-cli ./.loki/current ./.loki/reference ./.loki/difference -R ./.loki/report.html",
    "loki:ci": "concurrently -s first -k \"yarn storybook\" \"yarn loki:test\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
